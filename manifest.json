{
  "name": "PandoRdio",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "A Thumbs Up in Pandora adds the song to your Rdio Collection.",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "notifications"
  ],
  "web_accessible_resources": [
    "icon48.png"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.pandora.com/*"],
      "js": ["jquery.js", "script.js"]
    }
  ]
}
